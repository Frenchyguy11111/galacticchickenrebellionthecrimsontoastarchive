       local Tab = Window:CreateTab("Qte Exploit", "sword")
       local Section = Tab:CreateSection("WARNING, YOU CANNOT TURN THIS OFF")
       local Button = Tab:CreateButton({
   Name = "Inf Qte Strenght (WORKS ON ANY QTE)",
   Callback = function()
      local player = game.Players.LocalPlayer

while true do
    if player.Character and player.Character:FindFirstChild("Stats") then
        local clashStrength = player.Character.Stats:FindFirstChild("ClashStrength")
        if clashStrength then
            clashStrength.Value = 100
        end
    end
    task.wait(0.01)
end
   -- The function that takes place when the button is pressed
   end,
})
